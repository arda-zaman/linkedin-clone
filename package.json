  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel webpack-dev db-server",
    "build": "npm-run-all --parallel webpack-prod db-server",
    "webpack-dev": "webpack serve --mode development",
    "webpack-prod": "webpack --mode production",
    "db-server": "json-server --watch db.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "yarn run lint:js && yarn run lint:scss",
    "lint:js": "eslint src",
    "lint:scss": "npx stylelint '**/*.scss'",
    "husky-add-linter-precommit": "npx husky add .husky/pre-commit \"yarn lint\" && git add .husky/pre-commit"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.9",
    "@svgr/webpack": "^6.1.2",
    "babel-loader": "^8.2.3",
    "css-loader": "5.2.6",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.27.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.45.0",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "stylelint": "^14.1.0",
    "stylelint-scss": "^4.0.1",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "axios": "^0.24.0",
    "json-server": "^0.17.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  }
}
